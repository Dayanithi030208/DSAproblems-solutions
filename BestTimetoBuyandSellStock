class Solution {
    public int maxProfit(int[] prices) {
        int maxprof=0;
        int mini=prices[0];
        int cost=prices[0];
        for(int i=1;i<prices.length;i++){
            cost=prices[i]-mini;
            maxprof=Math.max(maxprof,cost);
            mini=Math.min(mini,prices[i]);
        }
        return maxprof;
    }
}
