class Solution {
    public int maxDepth(String s) {
        int parenthesiscnt=0;
        int n=s.length(); 
        int maxcnt=0;
        for(int i=0;i<n;i++){
            if(s.charAt(i)=='(') {
                parenthesiscnt++;
                maxcnt=Math.max(parenthesiscnt,maxcnt);
            }
            else if(s.charAt(i)==')') parenthesiscnt--;
        }
        return maxcnt;
    }
}
