class Solution {
    public int[] countBits(int n) {
        int[] Ans=new int[n+1];
        for(int i=0;i<=n;i++){
            int count=0;
            int num=i;
            while(num>0){
                count+=(num&1);
                num=(num>>1);
            }
            Ans[i]=count;
        }
        return Ans;
    }
}
