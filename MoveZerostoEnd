class Solution {
    public void moveZeroes(int[] nums) {
        int lastnon0=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i]!=0){
                nums[lastnon0]=nums[i];
                lastnon0++;
            }
        }
        for(int i=lastnon0;i<nums.length;i++) nums[i]=0;
    }
}
