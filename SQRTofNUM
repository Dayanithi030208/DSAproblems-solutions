class Solution {
    public long floorSqrt(long n) {
      long low=1;
      long high=n;
      long ans=1;
      while(low<=high){
        long mid=(low+high)/2;
        if(mid*mid==n){
            ans=mid;
            break;
        }
        if(mid*mid>n){
            high=mid-1;
        }
        else{
            low=mid+1;
            ans=mid;
        }
      }
      return ans;

    }
}
