class Solution {
    public int strStr(String haystack, String needle) {
        int len=needle.length();
        int n=haystack.length();
        for(int i=0;i<=n-len;i++){
            int start=i;
            int end=i+len;
            String sub=haystack.substring(start,end);
            if(needle.equals(sub)) return start;
        }
        return -1;
    }
}
