class Solution {
    public int[] successfulPairs(int[] spells, int[] potions, long success) {
        int n=spells.length;
        Arrays.sort(potions);
        int[] ans=new int[n];
        for(int i=0;i<n;i++){
            int s=spells[i];
            int low=0;
            int high=potions.length-1;
            int minindex=potions.length;
            while(low<=high){
                int mid=(low+high)/2;
                if((long)potions[mid]*s<success) low=mid+1;
                else{
                    minindex=mid;
                    high=mid-1;
                }
            }
            ans[i]=potions.length-minindex;
        }
        return ans;
    }
}
