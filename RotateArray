class Solution {
    public void rotate(int[] nums, int k) {
        if(k>nums.length) k=k%nums.length;
        k=nums.length-k;
        if(k==0) return;
        reverse(nums,0,k-1);
        reverse(nums,k,nums.length-1);
        reverse(nums,0,nums.length-1); 
    }
    static void reverse(int[] nums,int l,int r){
        while(l<r){
            int temp=nums[r];
            nums[r]=nums[l];
            nums[l]=temp;
            l++;
            r--;
        }
    }
}
